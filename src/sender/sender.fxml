<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" gridLinesVisible="true" hgap="10" prefHeight="143.0" prefWidth="173.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sender.Controller">
   <children>
      <ImageView fx:id="f_image" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="btn_load" minWidth="-Infinity" mnemonicParsing="false" onAction="#onclick_btn_load" text="загрузить" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
      <Button fx:id="btn_send" minWidth="-Infinity" mnemonicParsing="false" onAction="#onclick_btn_send" text="отправить" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" />

      <TextField fx:id="txt_reciever" promptText="адрес получателя" GridPane.rowIndex="1" />
      <ComboBox fx:id="cmb_users" onAction="#onaction_cmb_users" prefWidth="200.0" GridPane.rowIndex="2" />
      <Button fx:id="btn_account" mnemonicParsing="false" onAction="#onclick_btn_account" text="account" GridPane.rowIndex="3" />
   </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
